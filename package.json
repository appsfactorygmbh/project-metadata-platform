{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.3.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only -- {@}\" -- ",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix src",
    "format": "prettier --write .",
    "prepare": "husky",
    "upgrade:all": "yarn up \"*\"",
    "clean": "rm -rf coverage dist html cypress.config.js cypress.config.d.ts tsconfig.node.tsbuildinfo tsconfig.tsbuildinfo vite.config.js vite.config.d.ts stats.html",
    "clean:all": "yarn clean && rm -rf node_modules .million && yarn cache clean"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^11.1.0",
    "ant-design-vue": "^4.2.5",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "pinia": "^2.2.4",
    "splitpanes": "^3.1.5",
    "vue": "^3.5.12",
    "vue-auth3": "^1.0.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@pinia/testing": "^0.1.6",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/splitpanes": "^2.2.6",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/ui": "^2.1.3",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "antdv-component-resolver": "^1.0.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-gitlab": "^5.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.29.1",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.10",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^2.1.3",
    "vitest-sonar-reporter": "^2.0.0",
    "vue": "^3.5.12",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.8",
    "yarn-run-all": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,vue}": "eslint --fix",
    "**/*": "prettier -w -u"
  }
}
