{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.5.3",
  "scripts": {
    "vite:env": "LC_ALL=de_DE.UTF-8 TZ=UTC vite",
    "vitest:env": "LC_ALL=de_DE.UTF-8 TZ=UTC vitest",
    "dev": "yarn vite:env",
    "build": "run-p type-check \"build-only -- {@}\" -- ",
    "preview": "yarn vite:env preview",
    "test": "yarn vitest:env",
    "test:ui": "yarn vitest:env --ui",
    "coverage": "yarn vitest:env run --coverage",
    "build-only": "yarn vite:env build",
    "type-check": "vue-tsc --build --declaration --emitDeclarationOnly tsconfig.json",
    "type-build": "vue-tsc --build --force tsconfig.json",
    "lint:check": "eslint --config eslint.config.ts",
    "lint": "yarn lint:check --fix",
    "format": "prettier --write .",
    "prepare": "husky",
    "upgrade:all": "yarn up \"*\"",
    "clean": "rm -rf coverage dist html lib cypress.config.js cypress.config.d.ts tsconfig.node.tsbuildinfo tsconfig.tsbuildinfo vite.config.js vite.config.d.ts stats.html",
    "clean:all": "yarn clean && rm -rf node_modules .million && yarn cache clean",
    "openapi:generate": "TS_POST_PROCESS_FILE=\"./openapi-post-process.sh\" && openapi-generator-cli generate && ./openapi-post-process.sh ./src/api/generated/runtime.ts && yarn lint"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.0",
    "@ant-design/icons-vue": "^7.0.1",
    "@heroicons/vue": "^2.2.0",
    "@vueuse/core": "^12.5.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.9",
    "pinia": "2.3.0",
    "pinia-generic": "^0.2.0",
    "splitpanes": "^3.1.8",
    "vue": "^3.5.13",
    "vue-auth3": "^1.0.11",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@openapitools/openapi-generator-cli": "^2.16.3",
    "@pinia/testing": "^0.1.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.15",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.12.0",
    "@types/splitpanes": "^2.2.6",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "antdv-component-resolver": "^1.0.7",
    "cypress": "^14.0.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-formatter-gitlab": "^5.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^16.7.3",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.22.0",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.4",
    "vitest-sonar-reporter": "^2.0.0",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "2.2.0",
    "yarn-run-all": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,vue}": "eslint --fix",
    "**/*": "prettier -w -u"
  }
}
